<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>{{= it.name }}</id>
    <title>{{= it.title }}</title>
    <version>{{= it.version }}</version>
    <authors>{{= it.authors }}</authors>
    <owners>{{= it.owners }}</owners>
    <iconUrl>{{= it.iconUrl }}</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>{{= it.description }}</description>
  </metadata>
  <files>
    <file src="locales\**" target="lib\net45\locales" />
    <file src="resources\**" target="lib\net45\resources" />
    <file src="*.bin" target="lib\net45" />
    <file src="*.dll" target="lib\net45" />
    <file src="*.pak" target="lib\net45" />
    <file src="icudtl.dat" target="lib\net45\icudtl.dat" />
    <file src="LICENSE" target="lib\net45\LICENSE" />
    <file src="{{= it.exe }}" target="lib\net45\{{= it.exe }}" />
  </files>
</package>
